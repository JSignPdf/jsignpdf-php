{
  "name": "jsignpdf/jsignpdf-php",
  "description": "jsignpdf-php",
  "keywords": [
    "JSignPdf",
    "PHP",
    "Signature",
    "PHP Signature",
    "PDF",
    "PDF Sign PHP"
  ],
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Jeidison Farias",
      "email": "jeidison.farias@gmail.com"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": "^7.4|^8.0",
    "ext-openssl": "*",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5"
  },
  "autoload": {
    "psr-4": {
      "Jeidison\\JSignPDF\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Jeidison\\JSignPDF\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test:unit": "vendor/bin/phpunit --no-coverage --colors=always --fail-on-warning --fail-on-risky",
    "test:coverage": "XDEBUG_MODE=coverage vendor/bin/phpunit"
  }
}
